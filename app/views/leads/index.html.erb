<p id="notice"><%= notice %></p>

<h1>Leads</h1>

<table>
  <thead>
    <tr>
      <th>Full name of the contact</th>
      <th>Company name</th>
      <th>email</th>
      <th>Phone</th>
      <th>Project name</th>
      <th>project description</th>
      <th>Department in charge of the elevators</th>
      <th>Message</th>
      <th>Attached file</th>
      <th>Date of the contact</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.full_name_of_the_contact %></td>
        <td><%= lead.company_name %></td>
        <td><%= lead.email %></td>
        <td><%= lead.phone %></td>
        <td><%= lead.project_name %></td>
        <td><%= lead.project_description %></td>
        <td><%= lead.department_in_charge_of_the_elevators %></td>
        <td><%= lead.message %></td>
        <td><%= lead.attached_file %></td>
        <td><%= lead.date_of_the_contact %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>


